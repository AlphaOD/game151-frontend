{
  "name": "game151-frontend",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "web",
    "native"
  ],
  "scripts": {
    "dev:web": "cd web && npm run dev",
    "dev:native": "cd native && npm run start",
    "build:web": "cd web && npm run build",
    "build:native": "cd native && npm run build",
    "test": "npm run test:web && npm run test:native",
    "test:web": "cd web && npm test",
    "test:native": "cd native && npm test",
    "lint": "npm run lint:web && npm run lint:native",
    "lint:web": "cd web && npm run lint",
    "lint:native": "cd native && npm run lint",
    "type-check": "npm run type-check:web && npm run type-check:native",
    "type-check:web": "cd web && npm run type-check",
    "type-check:native": "cd native && npm run type-check"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^7.0.1"
  }
} 